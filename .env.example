# Solana Memecoin Trading Bot - Environment Configuration
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!

# WALLET CONFIGURATION
# Private keys in base58 format, comma-separated (NO SPACES)
# Generate using: cd ../solana-wallet-generator && node export-to-env.js
WALLET_PRIVATE_KEYS=

# SOLANA RPC
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_RPC_WS=wss://api.devnet.solana.com
SOLANA_NETWORK=devnet

# RISK MANAGEMENT
MAX_POSITION_SIZE_SOL=0.1
MAX_TOTAL_EXPOSURE_SOL=1.0
DEFAULT_SLIPPAGE_BPS=300

# SAFETY
DRY_RUN_MODE=true
LOG_LEVEL=info
